{
  "name": "joshoangtien/repo-design-pattern",
  "description": "Laravel 5|6|7|8|9 - Repositories to  the database layer",
  "keywords": [
    "laravel",
    "repository",
    "eloquent",
    "model",
    "cache"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Anderson Andrade",
      "homepage": "https://lamsaodecode.blogspot.com",
      "email": "jos.hoangtien@gmail.com",
      "role": "Developer"
    }
  ],
  "homepage": "http://andersao.github.io/repo-design-pattern",
  "support": {
    "email": "jos.hoangtien@gmail.com",
    "issues":"https://github.com/andersao/repo-design-pattern/issues",
    "wiki":"https://github.com/andersao/repo-design-pattern",
    "source":"https://github.com/andersao/repo-design-pattern",
    "docs": "http://andersao.github.io/repo-design-pattern"
  },
  "require": {
    "illuminate/http": "~5.0|~6.0|~7.0|^8.0|^9.0",
    "illuminate/config": "~5.0|~6.0|~7.0|^8.0|^9.0",
    "illuminate/support": "~5.0|~6.0|~7.0|^8.0|^9.0",
    "illuminate/database": "~5.0|~6.0|~7.0|^8.0|^9.0",
    "illuminate/pagination": "~5.0|~6.0|~7.0|^8.0|^9.0",
    "illuminate/console": "~5.0|~6.0|~7.0|^8.0|^9.0",
    "illuminate/filesystem": "~5.0|~6.0|~7.0|^8.0|^9.0",
    "joshoangtien/laravel-validation": "~1.1|~1.2|~1.3|~1.4",
    "illuminate/validation": "~5.0|~6.0|~7.0|^8.0|^9.0"
  },
  "autoload": {
    "psr-4": {
      "JosHoangTien\\Repository\\": "src/JosHoangTien/Repository/"
    }
  },
  "config": {
    "preferred-install": "dist"
  },
  "suggest": {
    "league/fractal": "Required to use the Fractal Presenter (0.12.*).",
    "robclancy/presenter": "Required to use the Presenter Model (1.3.*)",
    "joshoangtien/laravel-validation": "Required to provide easy validation with the repository (1.1.*)"
  },
  "extra": {
    "laravel": {
      "providers": [
        "JosHoangTien\\Repository\\Providers\\RepositoryServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable"
}
